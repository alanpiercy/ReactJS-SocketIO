{
    "env": {
        "es6": true,
        "node": true,
        "browser": true,
        "jquery": true,
        "mocha": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }        
    },
    "rules": {
        "no-console": 0,
        "quotes": ["error", "single"]
    }
}